{
    "parserOptions": {
        "ecmaVersion": 6
    },
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "no-require-imports": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "max-line-length": false
    },
    "rulesDirectory": []
}